#include <bits/stdc++.h>
#include <ext/pb_ds/assoc_container.hpp>
#include <ext/pb_ds/tree_policy.hpp>

using namespace std;
using namespace __gnu_pbds;

#pragma GCC optimize("Ofast, unroll-loops")
#pragma GCC target("avx2,fma")
#pragma GCC target("sse,sse2,sse3,ssse3,sse4.1,sse4.2,sse4a,abm,mmx,avx,popcnt,tune=native")

using ll = long long;
using pii = pair<int, int>;
using pll = pair<ll, ll>;
using vi = vector<int>;
using vll = vector<ll>;
using vb = vector<bool>;
using vpii = vector<pii>;
using vs = vector<string>;

#define psiblvdes                     \
    ios_base::sync_with_stdio(false); \
    cin.tie(nullptr);                 \
    cout.tie(nullptr);
#define pb push_back
#define st first
#define nd second
#define all(x) x.begin() + 1, x.end()
#define rall(x) x.rbegin(), x.rend()
#define sz(x) (ll) x.size()
#define watch(x) (cout << #x << " == " << x << '\n')

typedef tree<int, null_type, less<int>, rb_tree_tag, tree_order_statistics_node_update> ordered_set;

mt19937_64 rng(chrono::steady_clock::now().time_since_epoch().count());
random_device rd;
mt19937_64 mersenne(rd());

const int maxn = 2e5 + 7;
const int mod = 1e9 + 7;
const int mod2 = (119 << 23) + 1; // 998244353
const ll inf = 1e18;
const int iinf = 1e9;
const double eps = 1e-7;
const int block = 350;

void mtests(auto &x)
{
    int tests;
    cin >> tests;
    while (tests--)
    {
        x();
    }
}

template <typename T>
void readv(vector<T> &v, int from = 0)
{
    for (size_t i = from; i < v.size(); ++i)
    {
        cin >> v[i];
    }
}

template <typename T>
void printv(vector<T> &v, int from = 0)
{
    for (size_t i = from; i < v.size(); ++i)
    {
        cout << v[i] << ' ';
    }
}

void precalc()
{
}

void solve()
{
    int a, b;
    cin >> a >> b;
    int qweqwe = 100;
    for (int i = a; i <= b; ++i)
    {
        if ((i - a) + (b - i) < qweqwe)
        {
            qweqwe = (i - a) + (b - i);
        }
    }
    cout << qweqwe << '\n';
}

int main()
{
    psiblvdes;
    precalc();
    mtests(solve);
    //    solve();
}
